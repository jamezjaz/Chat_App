<div class="container my-4">
  <h5> Hi <%= @current_user.username %>, </h5>
  <h4> You are welcome </h4>

  <h4 class="mt-4"> Users </h4>
  <%= turbo_stream_from "users" %>
  <div id="users">
    <%= render @users %>
  </div>

  <h4 class="mt-4"> Rooms </h4>
  <%= turbo_stream_from "rooms" %>
  <div id="rooms">
    <%= render @rooms %>
  </div>
</div>